{
  "name": "surveys",
  "version": "1.0.0",
  "description": "surveys",
  "private": true,
  "scripts": {
    "build": "webpack -v && yarn run prod",
    "clean": "rimraf ./build",
    "prod": "cross-env NODE_ENV=production webpack --config ./webpack/prod.js",
    "dev": "cross-env NODE_HOST=localhost:8888 webpack-dev-server --config ./webpack/dev.js --profile --colors",
    "dll": "cross-env NODE_ENV=production webpack --config ./webpack/dll.js",
    "flow:test": "flow; test $? -eq 0 -o $? -eq 2",
    "flow": "flow",
    "start": "webpack-dev-server --config ./webpack/dev.js --profile --colors",
    "stage": "node webpack/server",
    "test": "jest --color",
    "test:watch": "jest --watch --color",
    "test:coverage": "jest --coverage --color"
  },
  "dependencies": {
    "babel-polyfill": "^6.16.0",
    "babel-runtime": "^6.25.0",
    "colors": "^1.1.2",
    "d3": "^4.10.0",
    "font-awesome": "^4.7.0",
    "history": "^4.6.3",
    "lodash": "^4.17.2",
    "moment": "^2.18.1",
    "moment-timezone": "^0.5.13",
    "path": "^0.12.7",
    "query-string": "^4.3.2",
    "react": "^15.6.1",
    "react-addons-css-transition-group": "^15.6.0",
    "react-datepicker": "^1.0.4",
    "react-dom": "^15.6.1",
    "react-i18nify": "^1.8.7",
    "react-redux": "^4.4.5",
    "react-redux-i18n": "^1.9.0",
    "react-router-dom": "^4.1.2",
    "react-router-redux": "^5.0.0-alpha.6",
    "react-select": "^1.0.0-rc.3",
    "react-toolbox": "^2.0.0-beta.12",
    "redux": "^3.7.2",
    "redux-logger": "^2.7.4",
    "redux-thunk": "^2.1.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.5.1",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.1.0",
    "babel-jest": "^18.0.0",
    "babel-loader": "^7.1.0",
    "babel-plugin-module-resolver": "^2.3.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es2015-function-name": "^6.24.1",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-constant-elements": "^6.9.1",
    "babel-plugin-transform-react-inline-elements": "^6.8.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.2.10",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.16.0",
    "cross-env": "^3.1.3",
    "css-loader": "^0.28.4",
    "enzyme": "^2.9.1",
    "eslint": "^3.8.1",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-flowtype": "^2.35.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.8.0",
    "express": "^4.15.4",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "firebase": "^4.8.0",
    "flow-bin": "^0.41.0",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^18.0.0",
    "json-loader": "^0.5.7",
    "material-design-icons": "^3.0.1",
    "node-sass": "^4.5.3",
    "postcss": "^6.0.9",
    "postcss-cssnext": "^3.0.2",
    "postcss-each": "^0.10.0",
    "postcss-import": "^10.0.0",
    "postcss-loader": "^2.0.6",
    "postcss-modules-scope": "1.0.2",
    "precss": "^1.4.0",
    "react-hot-loader": "^3.0.0-beta.6",
    "sass-loader": "^6.0.5",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "webpack": "^3.5.2",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-dev-server": "^2.6.1",
    "webpack-hot-middleware": "^2.18.2"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "browsers": [
              "last 2 versions",
              "safari >= 7"
            ]
          },
          "debug": true,
          "modules": false,
          "loose": true
        }
      ],
      "react"
    ],
    "plugins": [
      [
        "transform-runtime",
        {
          "polyfill": false,
          "regenerator": true,
          "moduleName": "babel-runtime"
        }
      ],
      "transform-flow-strip-types",
      "transform-es2015-function-name",
      "transform-react-remove-prop-types",
      "transform-react-constant-elements",
      "transform-react-inline-elements",
      "transform-object-rest-spread",
      "transform-class-properties"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "airbnb",
    "env": {
      "browser": true,
      "node": true,
      "amd": true,
      "jest": true,
      "es6": true
    },
    "plugins": [
      "react",
      "flowtype",
      "jsx-a11y"
    ],
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "rules": {
      "linebreak-style": 0,
      "arrow-body-style": 0,
      "comma-dangle": [
        "error",
        "always-multiline"
      ],
      "no-underscore-dangle": 0,
      "indent": [
        "error",
        2,
        {
          "SwitchCase": 1
        }
      ],
      "func-style": [
        "error",
        "declaration",
        {
          "allowArrowFunctions": true
        }
      ],
      "max-len": 0,
      "newline-per-chained-call": 0,
      "no-console": 0,
      "no-return-assign": 0,
      "no-use-before-define": 0,
      "no-param-reassign": 0,
      "prefer-template": 2,
      "import/imports-first": 0,
      "import/newline-after-import": 0,
      "import/no-extraneous-dependencies": 0,
      "import/no-named-as-default": 0,
      "import/no-unresolved": 2,
      "import/prefer-default-export": 0,
      "jsx-a11y/aria-props": 2,
      "jsx-a11y/heading-has-content": 0,
      "jsx-a11y/href-no-hash": 2,
      "jsx-a11y/label-has-for": 2,
      "jsx-a11y/mouse-events-have-key-events": 2,
      "jsx-a11y/role-has-required-aria-props": 2,
      "jsx-a11y/role-supports-aria-props": 2,
      "jsx-a11y/no-static-element-interactions": 0,
      "jsx-a11y/anchor-has-content": [
        2,
        {
          "components": [
            ""
          ]
        }
      ],
      "react/jsx-filename-extension": 0,
      "react/jsx-no-target-blank": 0,
      "react/require-extension": 0,
      "react/self-closing-comp": 0,
      "react/forbid-prop-types": 0,
      "react/require-default-props": 0,
      "require-yield": 0,
      "react/sort-comp": [
        1,
        {
          "order": [
            "type-annotations",
            "static-methods",
            "/.*[t|T]imer.*/",
            "lifecycle",
            "render",
            "everything-else"
          ]
        }
      ],
      "flowtype/define-flow-type": 1,
      "flowtype/require-parameter-type": 0,
      "flowtype/require-return-type": [
        0,
        "always",
        {
          "annotateUndefined": "never"
        }
      ],
      "flowtype/space-after-type-colon": [
        1,
        "always"
      ],
      "flowtype/space-before-type-colon": [
        1,
        "never"
      ],
      "flowtype/type-id-match": [
        0,
        "^([A-Z][a-z0-9]+)+Type$"
      ],
      "flowtype/use-flow-type": 1,
      "flowtype/valid-syntax": 1
    },
    "settings": {
      "flowtype": {
        "onlyFilesWithFlowAnnotation": true
      }
    }
  }
}
